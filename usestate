import React, { useEffect, useState } from 'react'

function FunctionEffect1() {
    let [count, setCount] = useState(0)
        // useEffect() 

    return (<div>
    <h1>{count}</h1>
   <button onClick={()=>setCount(count+1)}>updtae state</button> 

        </div>
    )
}

export default FunctionEffect1
